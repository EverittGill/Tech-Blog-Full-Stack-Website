{{!-- includes all blog posts and navigation --}}



<p>homepage is rendering</p>



<!-- Loop over each object in the projects array -->
{{#each article}}  
{{!-- as |article| --}}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{!-- {{{get_emoji}}} --}}
      {{!-- <a href="/articleData/{{article.id}}">{{article.title}}</a> --}}
      <a href="/article/{{id}}">
        <h3>{{title}}</h3> </a>
        <p>{{content}}</p>
        <p>Written by {{author}}</p>
    </h2>
    <p>
      <span for="img" aria-label="money"></span>
      <!-- Pass needed_funding value to the helper function -->
     
    </p>
    <p>Created by {{article.author}} on {{ article.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{!-- {{project.description}} --}}
    </p>
  </div>
</div>
{{/each}}


